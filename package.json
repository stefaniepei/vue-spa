{
  "name": "vue-spa",
  "version": "1.0.0",
  "description": "vue spa",
  "main": "App.js",
  "scripts": {
    "clean": "rimraf dist && mkdirp dist",
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "start": "npm run dev",
    "build:dev": "npm run clean && cross-env MY_ENV=dev npm run build",
    "build:test": "npm run clean && cross-env MY_ENV=test npm run build",
    "build:master": "npm run clean && cross-env MY_ENV=prod npm run build",
    "lint": "eslint --quiet --ext .js src",
    "lint:fix": "eslint --quiet --fix --ext .js src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stefaniepei/vue-spa.git"
  },
  "keywords": [
    "vue",
    "vuex",
    "vue-router",
    "spa"
  ],
  "author": "stefaniepei",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stefaniepei/vue-spa/issues"
  },
  "homepage": "https://github.com/stefaniepei/vue-spa#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "element-ui": "^2.6.1",
    "vue": "^2.6.9",
    "vue-router": "^3.0.2",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.5.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.15.2",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-vue": "^5.2.2",
    "file-loader": "^3.0.1",
    "husky": "^1.3.1",
    "node-sass": "^4.11.0",
    "postcss": "^7.0.14",
    "postcss-loader": "^3.0.0",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "vue-loader": "^15.7.0",
    "vue-style-loader": "^4.1.2",
    "webpack": "^4.29.6",
    "webpack-dev-server": "^3.2.1"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
